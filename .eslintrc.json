{
	"env": {
		"browser": true,
		"node": true

	},
	"parser": "@babel/eslint-parser",
	"parserOptions": {
		"requireConfigFile": false
	},

	"plugins": ["prettier"],
	"extends": ["prettier", "eslint:recommended", "plugin:prettier/recommended"],
	"rules": {
		"endOfLine": "auto",
		"indent": ["error", "tab"],
		"linebreak-style": ["error", "windows"]
	}
}
